<div id="content">
  <h2><u><%= title %></u></h2>
  <br />
  <% if (items.length > 0) { %>
  <table class="center">
    <thead>
      <th>id</th>
      <th>description</th>
    </thead>
    <tbody>
      <% for (let item of items) { %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.description %></td>
      </tr>
      <% } %>
    </tbody>
  </table>
  <% } %>
</div>
